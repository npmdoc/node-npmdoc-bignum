{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "bignum",
    "description": "#### api documentation for  [bignum (v0.12.5)](https://github.com/justmoon/node-bignum#readme)  [![npm package](https://img.shields.io/npm/v/npmdoc-bignum.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-bignum) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-bignum.svg)](https://travis-ci.org/npmdoc/node-npmdoc-bignum)",
    "devDependencies": {
        "bignum": "*",
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-bignum",
    "keywords": [
        "documentation",
        "bignum"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_bignum.js",
    "name": "npmdoc-bignum",
    "nameAlias": "npmdoc_bignum",
    "nameOriginal": "npmdoc-bignum",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-bignum"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.3"
}
